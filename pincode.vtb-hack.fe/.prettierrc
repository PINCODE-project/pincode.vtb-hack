{
  "semi": true,
  "doubleQuote": true,
  "parser": "typescript",
  "printWidth": 120,
  "tabWidth": 4,
  "useTabs": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "pluginSearchDirs": [
    "."
  ],
  "importOrder": [
    "./publicPath",
    "^@/(.*)$",
    "^[../]",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
}